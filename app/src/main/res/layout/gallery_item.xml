<com.zzh.gdut.gduthelper.view.widget.ScheduleRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="150dp"
    android:id="@+id/rl_schedule"
    android:layout_height="150dp"
 >

    <com.zzh.gdut.gduthelper.view.widget.ClickBackgroundView
        android:id="@+id/click_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true" />

    <TextView
        android:id="@+id/tv_schedule"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:layout_margin="@dimen/viewMargin_10dp"
        android:gravity="center"
        android:text="软件工程"
        android:textColor="@color/customWhite"
        android:textSize="@dimen/mainTestSize_16sp" />
</com.zzh.gdut.gduthelper.view.widget.ScheduleRelativeLayout>